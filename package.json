{
  "name": "@byjohann/utils",
  "type": "module",
  "version": "0.7.1",
  "packageManager": "pnpm@9.15.0",
  "description": "A collection of utilities for my projects",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/utils.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/utils/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./array": {
      "types": "./dist/array.d.mts",
      "default": "./dist/array.mjs"
    },
    "./csv": {
      "types": "./dist/csv.d.mts",
      "default": "./dist/csv.mjs"
    },
    "./is": {
      "types": "./dist/is.d.mts",
      "default": "./dist/is.mjs"
    },
    "./json": {
      "types": "./dist/json.d.mts",
      "default": "./dist/json.mjs"
    },
    "./lazy": {
      "types": "./dist/lazy.d.mts",
      "default": "./dist/lazy.mjs"
    },
    "./module": {
      "types": "./dist/module.d.mts",
      "default": "./dist/module.mjs"
    },
    "./object": {
      "types": "./dist/object.d.mts",
      "default": "./dist/object.mjs"
    },
    "./path": {
      "types": "./dist/path.d.mts",
      "default": "./dist/path.mjs"
    },
    "./string": {
      "types": "./dist/string.d.mts",
      "default": "./dist/string.mjs"
    },
    "./types": {
      "types": "./dist/types.d.mts",
      "default": "./dist/types.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@types/node": "^22.10.1",
    "bumpp": "^9.9.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "unbuild": "^3.0.0-rc.11",
    "vitest": "^2.1.8"
  }
}
