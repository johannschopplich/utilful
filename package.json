{
  "name": "utilful",
  "type": "module",
  "version": "2.1.0",
  "packageManager": "pnpm@10.11.0",
  "description": "A collection of TypeScript utilities",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/utilful#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/utilful.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/utilful/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./array": {
      "types": "./dist/array.d.ts",
      "default": "./dist/array.js"
    },
    "./csv": {
      "types": "./dist/csv.d.ts",
      "default": "./dist/csv.js"
    },
    "./defu": {
      "types": "./dist/defu.d.ts",
      "default": "./dist/defu.js"
    },
    "./emitter": {
      "types": "./dist/emitter.d.ts",
      "default": "./dist/emitter.js"
    },
    "./json": {
      "types": "./dist/json.d.ts",
      "default": "./dist/json.js"
    },
    "./module": {
      "types": "./dist/module.d.ts",
      "default": "./dist/module.js"
    },
    "./object": {
      "types": "./dist/object.d.ts",
      "default": "./dist/object.js"
    },
    "./path": {
      "types": "./dist/path.d.ts",
      "default": "./dist/path.js"
    },
    "./result": {
      "types": "./dist/result.d.ts",
      "default": "./dist/result.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "default": "./dist/string.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@types/node": "^22.15.21",
    "bumpp": "^10.1.1",
    "eslint": "^9.27.0",
    "tsdown": "^0.12.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
